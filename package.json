{
  "name": "skyline",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "start": "node_modules/.bin/webpack-dev-server --open --progress --watch",
    "build": "webpack",
    "test": "jest test/* -i --bail",
    "test:watch" "jest test/* --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kanenathan213/skyline.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kanenathan213/skyline/issues"
  },
  "jest": {
    "modulePaths": [
      "<rootDir>/js",
      "<rootDir>/node_modules"
    ],
    "resetModules": false,
    "transform": {
      "\\.(ts|js)x?$": "<rootDir>/node_modules/babel-jest"
    },
    "testRegex": "\\.spec\\.(ts|js)x?$",
    "unmockedModulePathPatterns": [
      "./node_modules/jasmine-reporters"
    ],
    "testEnvironment": "node",
    "collectCoverage": false,
    "collectCoverageFrom": [
      "js/**/*.{js,ts}"
    ],
    "coverageDirectory": "./coverage/"
  },
  "homepage": "https://github.com/kanenathan213/skyline#readme",
  "dependencies": {
    "humps": "^2.0.0",
    "keymirror": "^0.1.1",
    "lodash": "^4.17.4",
    "pubsub-js": "^1.5.6"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-preset-env": "^1.3.3",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^6.10.3",
    "jest": "^19.0.2",
    "jest-cli": "^19.0.2",
    "webpack": "^2.3.3",
    "webpack-dev-server": "^2.4.2"
  }
}
